{
    // AIOS Configuration
    "aios.core.enabled": true,
    "aios.context.persistAcrossRestarts": true,
    "aios.context.maxHistorySize": 1000,
    "aios.ai.pythonPath": "c:/dev/AIOS/ai/src",
    "aios.ai.corePath": "c:/dev/AIOS/core",
    "aios.debug.enabled": false,
    "aios.privacy.mode": "strict",
    "aios.network.enabled": false,
    "aios.telemetry.enabled": false,
    "aios.autoUpdate.enabled": false,
    "aios.marketplace.checkUpdates": false,
    "aios.logs.retention": "session-only",
    "aios.context.encryption": true,
    "aios.debug.traceLevel": "error-only",
    "telemetry.telemetryLevel": "off",
    // Python Configuration
    "python.analysis.extraPaths": [
        "./ai/src",
        "./ai/src/core",
        "./ai/src/core/integration"
    ],
    "python.analysis.autoImportCompletions": false,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.typeCheckingMode": "strict",
    "python.linting.enabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Args": [
        "--max-line-length=79",
        "--ignore=E203,W503"
    ],
    "python.formatting.provider": "none",
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestArgs": [
        "ai/tests",
        "-v"
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "python.terminal.activateEnvironment": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    // C# Configuration
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.enableImportCompletion": true,
    "omnisharp.organizeImportsOnFormat": false,
    "omnisharp.enableAsyncCompletion": true,
    "csharp.semanticHighlighting.enabled": true,
    "csharp.inlayHints.enableInlayHintsForParameters": true,
    "csharp.inlayHints.enableInlayHintsForLiteralParameters": true,
    "csharp.inlayHints.enableInlayHintsForIndexerParameters": true,
    "csharp.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "csharp.inlayHints.enableInlayHintsForOtherParameters": true,
    "csharp.inlayHints.enableInlayHintsForTypes": true,
    "csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
    "csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
    "csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
    // C++ Configuration
    "C_Cpp.default.intelliSenseMode": "windows-msvc-x64",
    "C_Cpp.default.compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/14.30.30705/bin/Hostx64/x64/cl.exe",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/core/include",
        "${workspaceFolder}/core/src",
        "${workspaceFolder}/core/build"
    ],
    "C_Cpp.default.defines": [
        "AIOS_DEBUG",
        "AIOS_DEVELOPMENT"
    ],
    "C_Cpp.clang_format_fallbackStyle": "Microsoft",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.suggestSnippets": true,
    // File Associations
    "files.associations": {
        "*.ainlp": "plaintext",
        "*.aios": "json",
        "*.xaml": "xml",
        "*.cs": "csharp",
        "*.cpp": "cpp",
        "*.hpp": "cpp",
        "*.h": "c",
        "*.c": "c",
        "*.py": "python",
        "*.md": "markdown",
        "CMakeLists.txt": "cmake",
        "*.cmake": "cmake"
    },
    // Editor Configuration
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {},
    "editor.rulers": [
        80,
        120
    ],
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.trimAutoWhitespace": true,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.inlayHints.enabled": "on",
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    // Search Configuration
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/bin": true,
        "**/obj": true,
        "**/build": true,
        "**/.venv": true,
        "**/venv": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/Debug": true,
        "**/Release": true,
        "**/x64": true,
        "**/x86": true,
        "**/.vs": true,
        "**/CMakeFiles": true,
        "**/*.vcxproj.user": true,
        "**/*.log": true,
        "**/*.tmp": true,
        "**/*.cache": true
    },
    // File Exclusions
    "files.exclude": {
        "**/.vscode/launch.json": false,
        "**/.vscode/tasks.json": false,
        "**/.vscode/settings.json": false,
        "**/.vscode/extensions.json": false,
        "**/.vscode/mcp.json": false,
        "**/.vscode/": false,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/bin": true,
        "**/obj": true,
        "**/Debug": true,
        "**/Release": true,
        "**/CMakeFiles": true,
        "**/*.vcxproj.user": true,
        "**/.vs": true,
        "**/node_modules": true,
        "**/*.exe": true,
        "**/*.dll": true,
        "**/*.pdb": true,
        "**/*.cache": true
    },
    // Logging Configuration
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.iconTheme": "vs-seti",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "readme",
    "workbench.tree.indent": 15,
    "workbench.tree.renderIndentGuides": "always",
    // Terminal Configuration
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt",
        "workbench.action.terminal.clear"
    ],
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "env": {
                "AIOS_HOME": "${workspaceFolder}",
                "AIOS_LOG_LEVEL": "DEBUG"
            }
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd",
            "env": {
                "AIOS_HOME": "${workspaceFolder}",
                "AIOS_LOG_LEVEL": "DEBUG"
            }
        }
    },
    "terminal.integrated.env.windows": {
        "AIOS_HOME": "${workspaceFolder}",
        "AIOS_LOG_LEVEL": "DEBUG",
        "PYTHONPATH": "${workspaceFolder}/ai/src"
    },
    // Git Configuration
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.autofetchPeriod": 180,
    "git.enableStatusBarSync": true,
    "git.decorations.enabled": true,
    "git.showPushSuccessNotification": true,
    "git.openRepositoryInParentFolders": "always",
    // AIOS Specific Settings
    "aios.pythonEnvironment.autoDiscovery": true,
    "aios.pythonEnvironment.healthCheckInterval": 300,
    "aios.pythonEnvironment.autoRecovery": true,
    "aios.fractalHolographic.enabled": true,
    "aios.fractalHolographic.contextPersistence": true,
    "aios.debugging.integration": true,
    "aios.debugging.contextSnapshots": true,
    "aios.logging.level": "DEBUG",
    "aios.logging.enableConsole": true,
    "aios.logging.enableFile": true,
    "aios.logging.filePath": "${workspaceFolder}/logs/aios-debug.log",
    "aios.build.autoRestore": true,
    "aios.build.showWarnings": true,
    "aios.build.parallelBuild": true,
    "aios.integration.enableWebView2": true,
    "aios.integration.enableCppPython": true,
    "aios.integration.enableCsharpCpp": true,
    "aios.ai.enableNLPDebug": true,
    "aios.ai.enablePredictiveAnalysis": true,
    "aios.ai.enableAutomation": true,
    "aios.ui.enableHybridMode": true,
    "aios.ui.enableHolographicUI": true,
    "aios.ui.enableFractalComponents": true,
    "aios.performance.enableProfiling": true,
    "aios.performance.enableMemoryMonitoring": true,
    "aios.performance.enablePerformanceCounters": true,
    "aios.security.enableSandbox": true,
    "aios.security.enableAuditLog": true,
    "aios.security.enableSecurityScanning": true,
    // Extension Settings
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.buildDirectory": "${workspaceFolder}/core/build",
    "cmake.generator": "Visual Studio 17 2022",
    "cmake.preferredGenerators": [
        "Visual Studio 17 2022",
        "Visual Studio 16 2019"
    ],
    "cmake.debugConfig": {
        "name": "AIOS Core Debug",
        "type": "cppvsdbg",
        "request": "launch"
    },
    "dotnetAcquisition.enableTelemetry": false,
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
    "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
    "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
    "dotnet.inlayHints.enableInlayHintsForTypes": true,
    "dotnet.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
    "dotnet.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
    "dotnet.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
    "dotnet.server.useOmnisharp": false,
    // Unified root solution (2025-08-17 harmonization): point to canonical root AIOS.sln
    "dotnet.defaultSolution": "${workspaceFolder}/AIOS.sln",
    "github.copilot.chat.defaultSystemMessage": "Apply Debug-First Principle. Check existing docs before creating new files.",
    "github.copilot.chat.includeFiles": [
        "docs/ai-context/AI_QUICK_REFERENCE.md",
        "AIOS_PROJECT_CONTEXT.md",
        "governance/hook_policy.json"
    ],
    "github.copilot.chat.systemMessageMaxTokens": 4096,
    "omnisharp.defaultLaunchSolution": "AIOS.sln",
    "cmake.sourceDirectory": "C:/dev/AIOS/core",
    // Copilot Chat UI controls (hide Files changed block and related edit context)
    "chat.checkpoints.showFileChanges": false,
    "github.copilot.chat.edits.temporalContext.enabled": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "accessibility.signals.terminalBell": {
        "sound": "off"
    }
}
