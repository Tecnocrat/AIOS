[MASTER]
ignore=__pycache__,node_modules,build,dist,.venv,.git,vscode-extension
jobs=2

[MESSAGES CONTROL]
disable=C0114,C0115,C0116,W0611,W0612,W0613,R0903,R0902,R0913,R0914,R0915

[REPORTS]
output-format=colorized
reports=no

[FORMAT]
max-line-length=120
indent-string='    '

[TYPECHECK]
ignore-mixin-members=yes

[DESIGN]
max-args=8
max-locals=20
max-branches=15
max-statements=50
max-parents=7
max-attributes=10

[LOGGING]
logging-modules=logging

[REFACTORING]
max-nested-blocks=5

[VARIABLES]
init-import=no

[EXCEPTIONS]
overgeneral-exceptions=Exception

[SIMILARITIES]
min-similarity-lines=10

[PYTHON3]
python-version=3.12

[Basic]
good-names=i,j,k,ex,Run,_,pk,id

[Miscellaneous]
notes=FIXME,XXX,TODO

[Spellchecking]
spelling-dict=en_US
spelling-ignore-words=aios,VSCode,FastAPI,uvicorn,pytest,TensorFlow,AINLP,pybind11,subprocess,asyncio,psutil,endpoint,handler,dispatcher,optimizer,cellular,ecosystem,context,bridge,automation,architecture,performance,optimization,debug,log,request,response,workspace,session,pattern,design,system,code,model,python,csharp,rust,cpp
spelling-private-dict-file=.pylintdict
spelling-store-unknown-words=yes

[RECURSION]
max-recursion-depth=5

[IMPORTS]
known-standard-library=asyncio,logging,os,subprocess,sys,traceback,datetime,psutil,fastapi,concurrent,threading,typing
known-third-party=pytest,uvicorn,pybind11
known-local-folder=ai,aios_vscode_integration,core,interface,config,docs,tools,scripts,runtime,languages

[EXTERNAL]
extension-pkg-whitelist=psutil,fastapi,uvicorn,pybind11

[PYLINT]
load-plugins=pylint.extensions.docparams,pylint.extensions.mccabe
