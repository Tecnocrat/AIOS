{
  "system": {
    "name": "AIOS",
    "version": "1.0.0",
    "description": "Artificial Intelligence Operating System",
    "core": {
      "maxThreads": 8,
      "memoryLimit": "8GB",
      "logLevel": "INFO",
      "enableProfiling": false
    },
    "ai": {
      "modelPath": "./ai/models/",
      "defaultModel": "gpt-3.5-turbo",
      "maxContextLength": 4096,
      "temperature": 0.7,
      "enableGpu": true,
      "batchSize": 32
    },
    "ui": {
      "theme": "dark",
      "language": "en-US",
      "enableAnimations": true,
      "refreshRate": 60
    },
    "integration": {
      "enableCppPythonBridge": true,
      "enableCsharpCppBridge": true,
      "apiPort": 8080,
      "enableWebInterface": true
    },
    "logging": {
      "level": "INFO",
      "enableFileLogging": true,
      "logPath": "./logs/",
      "maxLogSize": "100MB",
      "enableStructuredLogging": true
    },
    "security": {
      "enableSandbox": true,
      "allowUnsafeOperations": false,
      "enableAuditLog": true
    }
  },
  "features": {
    "naturalLanguageInterface": true,
    "predictiveResourceManagement": true,
    "contextAwareAutomation": true,
    "intelligentErrorRecovery": true,
    "continuousLearning": true
  },
  "paths": {
    "core": "./core/",
    "ai": "./ai/",
    "interface": "./interface/",
    "config": "./config/",
    "resources": "./resources/",
    "build": "./build/"
  }
}
